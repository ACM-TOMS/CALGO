<?xml version="1.0" encoding="utf-8" ?>
<NEKTAR>
	<!-- Embed a 2-dimensional object in a 2-dimensional space -->
	<!-- DIM <= SPACE -->
	<!-- This provides a method of optimizing code for a 1-D curve embedded in 3-space. -->
	<GEOMETRY DIM="2" SPACE="2">
		<!-- Definitions that can be used below in this file. -->
		<!-- Not currently implemented -->
		<DEF>
			A = 1.0
			B = 2.0
			C = 3.0
		</DEF>
		<VERTEX>
			<!-- Always must have four values per entry. -->
            <V ID="0"> 0.0 0.0 0.0 </V>
            <V ID="1"> 1.0 0.0 0.0 </V>
            <V ID="2"> 2.0 0.0 0.0 </V>
            <V ID="3"> 0.0 1.0 0.0 </V>
            <V ID="4"> 1.0 1.0 0.0 </V>
            <V ID="5"> 2.0 1.0 0.0 </V>
            <V ID="6"> 0.0 2.0 0.0 </V>
            <V ID="7"> 1.0 2.0 0.0 </V>
            <V ID="8"> 2.0 2.0 0.0 </V>
        </VERTEX>

        <!-- Edges are vertex pairs -->
        <EDGE>
            <E ID="0"> 0 1 </E>
            <E ID="1"> 1 2 </E>
            <E ID="2"> 0 3 </E>
            <E ID="3"> 0 4 </E>
            <E ID="4"> 1 4 </E>
            <E ID="5"> 2 4 </E>
            <E ID="6"> 2 5 </E>
            <E ID="7"> 3 4 </E>
            <E ID="8"> 4 5 </E>
            <E ID="9"> 3 6 </E>
            <E ID="10"> 4 6 </E>
            <E ID="11"> 4 7 </E>
            <E ID="12"> 4 8 </E>
            <E ID="13"> 5 8 </E>
            <E ID="14"> 6 7 </E>
            <E ID="15"> 7 8 </E>
        </EDGE>

        <!-- Can include Q - quads, T - triangles -->
        <FACE></FACE>

        <!-- Curves can be specified for edges in all dimensions and faces in 2D and higher. -->
        <!-- Specify edges with "E" tag and faces with "F" for face. -->
        <!-- The edge IDs are from the EDGE list and the IDs for from the FACE list are
		from the list of FACEs.  -->
        <CURVE>
            <E ID="0" EDGEID="0" TYPE="GLL" NUMPOINTS="2">
                4.3 2.98 4.6
                7.3 9.2 8.4
            </E>
            <E ID="1" EDGEID="2" TYPE="POLYEVEN" NUMPOINTS="2">
                4.3 2.98 4.6
                7.3 9.2 8.4
            </E>
            <F ID="0" FACEID="0" TYPE="FEKETE" NUMPOINTS="3">
                4.3 2.98 4.6
                7.3 9.2 8.4
                7.3 9.2 8.4
            </F>
            <F ID="1" FACEID="0" TYPE="GLL, GLL" NUMPOINTS="4">
                4.3 2.98 4.6
                7.3 9.2 8.4
                4.3 2.98 4.6
                7.3 9.2 8.4
            </F>
            <F ID="2" FACEID="0" TYPE="POLYEVEN, POLYEVEN" NUMPOINTS="4">
                4.3 2.98 4.6
                7.3 9.2 8.4
                4.3 2.98 4.6
                7.3 9.2 8.4
            </F>
            
        </CURVE>
        
        <!-- Q - quads, T - triangles, S - segments, E - tet, P - pyramid, R - prism, H - hex -->
		<!-- Only certain element types are appropriate for the given dimension (dim on mesh) -->
		<!-- Can also use faces to define 3-D elements.  Specify with F[1] for face 1, for example. -->
		<!-- Element need to be entered in consistent "anti-clockwise" manner -->
		<ELEMENT>
			<!-- List the edges for each element -->
			<T ID="0"> 3 7 2 </T>
			<T ID="1"> 0 4 3 </T>
			<T ID="2"> 1 5 4 </T>
            <T ID="3"> 6 8 5 </T>
            <T ID="4"> 7 10 9 </T>
            <T ID="5"> 11 14 10 </T>
            <T ID="6"> 11 12 15 </T>
            <T ID="7"> 8 13 12 </T>
		</ELEMENT>
                		
		<!-- V - vertex, E - edge, F - face, T - Triangle, Q - Quad -->
		<COMPOSITE>
			<C ID="0"> V[1-3] </C>
			<C ID="1"> E[5-7] </C>
			<C ID="2"> T[0-2] T[4] T[6-7] </C>
		</COMPOSITE>

        <DOMAIN> C[0] </DOMAIN>

    </GEOMETRY>

    <EXPANSIONS>
        <E COMPOSITE="C[0]" NUMMODES="5" TYPE="MODIFIED" />
    </EXPANSIONS>
    
</NEKTAR>
