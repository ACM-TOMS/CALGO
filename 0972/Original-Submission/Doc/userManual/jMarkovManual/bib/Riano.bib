@INPROCEEDINGS{silv.ea:IIE09,
  author = {Silva, Daniel F. and Amaya, Ciro and Riaño, Germán},
  title = {Continuous-Time Models for Estimating Picker Blocking in Order-Picking-Systems},
  booktitle = {Proceedings of the IIE Conference},
  year = {2009},
  abstract = {A frequent problem in manual order-picking-systems is congestion,
	we developed CTMC models to represent picker blocking in cases including
	pickers with different exponentially distributed picking times and
	k pickers, for narrow and wide aisle systems. The focus was not to
	produce an all-inclusive model, but to generate models that give
	designers insight of the system, allowing them to reduce the number
	of variables considered. We ran instances with various parameters
	and validated the models against simulation. We found stochastic
	speeds generate more blockage that deterministic ones, differences
	in pickers cause more congestion, systems with k pickers are more
	complex.},
  owner = {German},
  timestamp = {2010.05.16}
}

@CONFERENCE{rian.silva:InformsPractice09,
  author = {Silva, D. and Riano, G.},
  title = {{Kimberly-Clark} {L}atin {A}merica Solves a Transportation Problem
	with Help of an Optimization Contest},
  booktitle = {2010 Informs Conference on OR Practice},
  year = {2010},
  address = {l},
  abstract = {The Transport Load Builder is an optimization tool developed by Kimberly-Clark
	Latin America to address several facts in the company’s transport
	planning operations: (1) 56% of the shipments are LTL, (2) planning
	is a time-consuming process, and (3) truck utilization measures had
	been consistently low. Since the tool was launched in Lima, Peru,
	truck efficiency has increased 25%, and average shipping cost per
	cubic meter has been reduced by over 10%. Implementing the tool in
	other countries posed a challenging problem since operations there
	rely on both direct shipments and third party cross-docking operations.
	To help solve this challenge, Riaño and his team launched an optimization
	contest, in association with the Latin American Association of Operations
	Research (ALIO). There were 24 teams from universities all over the
	region and the final winner was a team from Universidad de Chile.
	Several of their ideas were incorporated into the solution, and implementation
	is under way. Co-authors: Daniel Silva, Gustavo Angulo, Diego Morán,
	Rodrigo López, Daniel Espinoza, Ph.D.},
  owner = {l31796},
  timestamp = {2011.07.15},
  url = {http://meetings2.informs.org/Practice2010/supplychain.htm}
}

