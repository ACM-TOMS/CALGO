@CONFERENCE{rian.goez:IERC07,
  author = {Riaño, Germán and Góez, Julio},
  title = {A Stochastic Model of Bucket Brigades Using Phase-Type Distributions},
  booktitle = {IERC},
  year = {2007},
  address = {Nashville, TN, USA},
  abstract = {In this paper, we present a model of Bucket Brigades systems when
	the work content is stochastic by using Markov chains and phase-type
	distributions. The modeling technique used allows us to model fairly
	general systems, and accurately represent blocking, queuing and the
	impact of having non-consistent workers sorted in different arrangements
	along the line. Our numerical experiments show that, contrary to
	recommendations in the literature, there are instances of the Bucket
	Brigades system where throughput can be increased by allowing small
	inventory buffers between the stations. Also, our model can be used
	to establish the operators order when they do not dominate each other
	consistently along the line.},
  owner = {german},
  timestamp = {2007.05.08}
}

