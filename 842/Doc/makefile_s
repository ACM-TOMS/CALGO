%.o : %.f
	$(F77) $(F77OPTS) -c $*.f


#
GMRES = sPackgmres.o blas_rot.o
all: sTestgmres sDriver
#
#
sTestgmres: sTestgmres.o $(GMRES) 
	$(F77) $(F77OPTS) -o $@ $@.o $(GMRES) $(BLAS)
#
sDriver: sDriver.o $(GMRES) 
	$(F77) $(F77OPTS) -o $@ $@.o $(GMRES) $(BLAS)

sPackgmres.o: ../../Src/sPackgmres.f
	$(F77) $(F77OPTS) -c ../../Src/sPackgmres.f
clean :
	/bin/rm -f *.o sTestgmres fort.10 sol_sTestgmres sDriver
