CUBPACK = /home/ronald/Cubpack++

MYTESTPROG = vb7.o

TARGET = tst

#########################################################

LIB_DIR=$(CUBPACK)/Src
INCLUDE_DIR=$(CUBPACK)/Src

CC  =   CC

CFLAGS = -I$(INCLUDE_DIR)

libdir =  -L$(LIB_DIR)
libs =  -lcubpack -lm 

.c.o: ; $(CC) $(CFLAGS) -c  $*.c


$(TARGET):  $(MYTESTPROG)
	$(CC) $(MYTESTPROG) $(libdir) $(libs) -o $(TARGET)

clean:
	/bin/rm -f *.o $(TARGET) core
