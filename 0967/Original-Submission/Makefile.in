install-lib_libfmm_aHEADERS: $(lib_libfmm_a_HEADERS)
	@$(NORMAL_INSTALL)
	test -z "$(lib_libfmm_adir)" || $(MKDIR_P) "$(DESTDIR)$(lib_libfmm_adir)"
	@list='$(lib_libfmm_a_HEADERS)'; test -n "$(lib_libfmm_adir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_HEADER) $$files '$(DESTDIR)$(lib_libfmm_adir)'"; \
	  $(INSTALL_HEADER) $$files "$(DESTDIR)$(lib_libfmm_adir)" || exit $$?; \
	done

uninstall-lib_libfmm_aHEADERS:
	@$(NORMAL_UNINSTALL)
	@list='$(lib_libfmm_a_HEADERS)'; test -n "$(lib_libfmm_adir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(lib_libfmm_adir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(lib_libfmm_adir)" && rm -f $$files

