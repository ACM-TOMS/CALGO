# Makefile for lsa

all: teqlu teqnu tgacu tgacs tgadu tgads tlipu tlips tlisu tliss tmaxu\
     tnecu tnecs tnedu tneds tnetu tnets tsecu tsecs tsedu tseds tsenu tsumu

peql.o: peql.f
	gfortran -c peql.f

peqn.o: peqn.f
	gfortran -c peqn.f

pgac.o: pgac.f
	gfortran -c pgac.f

pgad.o: pgad.f
	gfortran -c pgad.f

plip.o: plip.f
	gfortran -c plip.f

plis.o: plis.f
	gfortran -c plis.f

pmax.o: pmax.f
	gfortran -c pmax.f

pnec.o: pnec.f
	gfortran -c pnec.f

pned.o: pned.f
	gfortran -c pned.f

pnet.o: pnet.f
	gfortran -c pnet.f

psec.o: psec.f
	gfortran -c psec.f

psed.o: psed.f
	gfortran -c psed.f

psen.o: psen.f
	gfortran -c psen.f

psum.o: psum.f
	gfortran -c psum.f

mssubs.o: mssubs.f
	gfortran -c mssubs.f

pssubs.o: pssubs.f
	gfortran -c pssubs.f

tssubs.o: tssubs.f
	gfortran -c tssubs.f

teqlu: teqlu.f peql.o mssubs.o pssubs.o tssubs.o
	gfortran -o teqlu teqlu.f peql.o mssubs.o pssubs.o tssubs.o

teqnu: teqnu.f peqn.o mssubs.o pssubs.o tssubs.o
	gfortran -o teqnu teqnu.f peqn.o mssubs.o pssubs.o tssubs.o

tgacu: tgacu.f pgac.o mssubs.o pssubs.o tssubs.o
	gfortran -o tgacu tgacu.f pgac.o mssubs.o pssubs.o tssubs.o

tgacs: tgacs.f pgac.o mssubs.o pssubs.o tssubs.o
	gfortran -o tgacs tgacs.f pgac.o mssubs.o pssubs.o tssubs.o

tgadu: tgadu.f pgad.o mssubs.o pssubs.o tssubs.o
	gfortran -o tgadu tgadu.f pgad.o mssubs.o pssubs.o tssubs.o

tgads: tgads.f pgad.o mssubs.o pssubs.o tssubs.o
	gfortran -o tgads tgads.f pgad.o mssubs.o pssubs.o tssubs.o

tlipu: tlipu.f plip.o mssubs.o pssubs.o tssubs.o
	gfortran -o tlipu tlipu.f plip.o mssubs.o pssubs.o tssubs.o

tlips: tlips.f plip.o mssubs.o pssubs.o tssubs.o
	gfortran -o tlips tlips.f plip.o mssubs.o pssubs.o tssubs.o

tlisu: tlisu.f plis.o mssubs.o pssubs.o tssubs.o
	gfortran -o tlisu tlisu.f plis.o mssubs.o pssubs.o tssubs.o

tliss: tliss.f plis.o mssubs.o pssubs.o tssubs.o
	gfortran -o tliss tliss.f plis.o mssubs.o pssubs.o tssubs.o

tmaxu: tmaxu.f pmax.o mssubs.o pssubs.o tssubs.o
	gfortran -o tmaxu tmaxu.f pmax.o mssubs.o pssubs.o tssubs.o

tnecu: tnecu.f pnec.o mssubs.o pssubs.o tssubs.o
	gfortran -o tnecu tnecu.f pnec.o mssubs.o pssubs.o tssubs.o

tnecs: tnecs.f pnec.o mssubs.o pssubs.o tssubs.o
	gfortran -o tnecs tnecs.f pnec.o mssubs.o pssubs.o tssubs.o

tnedu: tnedu.f pned.o mssubs.o pssubs.o tssubs.o
	gfortran -o tnedu tnedu.f pned.o mssubs.o pssubs.o tssubs.o

tneds: tneds.f pned.o mssubs.o pssubs.o tssubs.o
	gfortran -o tneds tneds.f pned.o mssubs.o pssubs.o tssubs.o

tnetu: tnetu.f pnet.o mssubs.o pssubs.o tssubs.o
	gfortran -o tnetu tnetu.f pnet.o mssubs.o pssubs.o tssubs.o

tnets: tnets.f pnet.o mssubs.o pssubs.o tssubs.o
	gfortran -o tnets tnets.f pnet.o mssubs.o pssubs.o tssubs.o

tsecu: tsecu.f psec.o mssubs.o pssubs.o tssubs.o
	gfortran -o tsecu tsecu.f psec.o mssubs.o pssubs.o tssubs.o

