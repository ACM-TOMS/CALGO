%.o : %.f
	$(F77) $(F77OPTS) -c $*.f

#
GMRES = zPackgmres.o blas_rot.o
all: zTestgmres zDriver
#
#
zTestgmres: zTestgmres.o $(GMRES) 
	$(F77) $(F77OPTS) -o $@ $@.o $(GMRES) $(BLAS)
#
zDriver: zDriver.o $(GMRES) 
	$(F77) $(F77OPTS) -o $@ $@.o $(GMRES) $(BLAS)

zPackgmres.o: ../../Src/zPackgmres.f
	$(F77) $(F77OPTS) -c ../../Src/zPackgmres.f
clean :
	/bin/rm -f *.o zTestgmres fort.30 sol_zTestgmres zDriver
