%.o : %.f
	$(F77) $(F77OPTS) -c $*.f


#
GMRES = dPackgmres.o blas_rot.o
all: dTestgmres dDriver
#
#
dTestgmres: dTestgmres.o $(GMRES) 
	$(F77) $(F77OPTS) -o $@ $@.o $(GMRES) $(BLAS)
#
dDriver: dDriver.o $(GMRES) 
	$(F77) $(F77OPTS) -o $@ $@.o $(GMRES) $(BLAS)

dPackgmres.o: ../../Src/dPackgmres.f
	$(F77) $(F77OPTS) -c ../../Src/dPackgmres.f
clean :
	/bin/rm -f *.o dTestgmres fort.20 sol_dTestgmres dDriver
