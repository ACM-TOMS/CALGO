%.o : %.f
	$(F77) $(F77OPTS) -c $*.f

#
GMRES = cPackgmres.o blas_rot.o
all: cTestgmres cDriver
#
#
cTestgmres: cTestgmres.o $(GMRES) 
	$(F77) $(F77OPTS) -o $@ $@.o $(GMRES) $(BLAS)
#
cDriver: cDriver.o $(GMRES) 
	$(F77) $(F77OPTS) -o $@ $@.o $(GMRES) $(BLAS)

cPackgmres.o: ../../Src/cPackgmres.f
	$(F77) $(F77OPTS) -c ../../Src/cPackgmres.f
clean :
	/bin/rm -f *.o cTestgmres fort.30 sol_cTestgmres cDriver
